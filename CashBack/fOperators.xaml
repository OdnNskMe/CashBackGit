<Window x:Name="FormOperators" x:Class="CashBack.fOperators"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CashBack"
        mc:Ignorable="d"
        Title="fOperator" Height="300" Width="374">
    <Grid>
        <ToolBarPanel HorizontalAlignment="Left" Height="30" Margin="10,10,0,0" VerticalAlignment="Top" Width="240" Orientation="Horizontal">
            <Button x:Name="bOperatorAdd" Content="Добавить" Height="30" VerticalAlignment="Top" Width="75" Click="bOperatorAdd_Click"/>
            <Button x:Name="bOperatorEdit" Content="Редактировать" Height="30" VerticalAlignment="Top" Width="90" Click="bOperatorEdit_Click"/>
            <Button Content="Удалить" Height="30" VerticalAlignment="Top" Width="75"/>
        </ToolBarPanel>
        <GroupBox Header="Операторы" HorizontalAlignment="Left" Height="204" Margin="15,55,0,0" VerticalAlignment="Top" Width="341">
            <Grid HorizontalAlignment="Left" Height="180" Margin="10,0,0,0" VerticalAlignment="Top" Width="310"/>
        </GroupBox>

    </Grid>
</Window>
