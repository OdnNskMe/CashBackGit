<Window x:Name="FormOperatorSeting" x:Class="CashBack.fOperatorSet"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CashBack"
        mc:Ignorable="d"
        Title="fOperatorSet" Height="300" Width="300">
    <Grid>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="60,61,0,0" TextWrapping="Wrap" Text="TextBoxLogin" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="60,120,0,0" TextWrapping="Wrap" Text="TextBoxFIO" VerticalAlignment="Top" Width="120"/>
        <Label Content="Login" HorizontalAlignment="Left" Margin="60,30,0,0" VerticalAlignment="Top"/>
        <Label Content="FIO" HorizontalAlignment="Left" Margin="60,89,0,0" VerticalAlignment="Top"/>
        <Button Content="Ok" HorizontalAlignment="Left" Margin="150,180,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="50,180,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
