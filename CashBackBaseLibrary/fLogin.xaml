<Window x:Class="CashBackBaseLibrary.fLogin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CashBackBaseLibrary"
        mc:Ignorable="d"
        Title="fLogin" Height="300" Width="300">
    <Grid>
        <PasswordBox HorizontalAlignment="Left" Margin="75,168,0,0" VerticalAlignment="Top" Width="165"/>
        <Label Content="Login" HorizontalAlignment="Left" Margin="85,89,0,0" VerticalAlignment="Top" Width="155"/>
        <Button Content="Connect" HorizontalAlignment="Left" Margin="165,205,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="bConnectSet" Content="Set" HorizontalAlignment="Left" Margin="207,50,0,0" VerticalAlignment="Top" Width="33" Click="bConnectSet_Click"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="85,142,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="75,120,0,0" TextWrapping="Wrap" Text="TextBoxLogin" VerticalAlignment="Top" Width="165"/>

    </Grid>
</Window>
